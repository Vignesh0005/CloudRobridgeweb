# Robridge AI Integration Environment Variables
# Copy this file to .env and update with your actual values

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Server Configuration
AI_SERVER_PORT=5000
WEB_SERVER_PORT=3001

# ESP32 Configuration (update these in your Arduino code)
ESP32_SERVER_IP=your-laptop-ip-here
ESP32_WIFI_SSID=your-wifi-ssid
ESP32_WIFI_PASSWORD=your-wifi-password
