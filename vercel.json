{
  "version": 2,
  "builds": [
    {
      "src": "Robridge web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "Robridge web/api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Robridge web/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Robridge web/build/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CI": "false"
  }
}
